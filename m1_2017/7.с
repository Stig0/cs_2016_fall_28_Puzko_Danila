#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int result=0, i=0;
    int a=0;
    char z;
    int b=0;

    scanf("%d%c%d", &a, &z, &b);

    switch(z)
    {
    case '+':
        result=a+b;
        printf("res=%d", result);
        break;

    case '-':
        result=a-b;
        printf("res=%d", result);
        break;

    case '*':
        result=a*b;
        printf("res=%d", result);
        break;

    case '/':
        result=a/b;
        printf("res=%d", result);
        break;

    case '^':
        result=pow(a,b);
        printf("res=%d", result);
        break;

    default:
        printf("Error");




    }
    return 0;
}
