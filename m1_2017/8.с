#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int a=0, b=0, c=0;
    float d=0, x1=0, x2=0, x=0;
    printf("Vvedite a, b, c bez zapyatoy:");
    scanf("%d %d %d", &a, &b, &c);
    printf("%d\n", a);
    printf("%d\n", b);
    printf("%d\n", c);
    d=b*b-4*a*c;
    printf("%f\n", d);
    if (d>0)
        {
            x1=(-b+sqrt(d))/(2*a);
            x2=(-b-sqrt(d))/(2*a);
            printf("x1=%f, x2=%f",x1, x2);
    }
    else if(d==0)
        {
            x=-b/(2*a);
    printf("x=%f", x);
    }
    else{
        printf("Veschestvennyh kornei net");
    }
    return 0;
}
